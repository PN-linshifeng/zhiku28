{
    "presets": [
        [
            "env",
            {
                "targets":
                {
                    "browsers": ["last 10 versions"]
                }
            }
        ],
        "stage-2",
        "react"
    ],
    "plugins": [
        ["transform-runtime",
        {
            "helpers": true,
            "polyfill": true,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }], "transform-decorators-legacy", "react-hot-loader/babel"
    ]
}